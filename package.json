{
  "name": "frau-module-loader",
  "version": "0.1.0",
  "description": "Utility to allow free-range apps to require external modules in browserify without needing to set up a shim",
  "main": "index.js",
  "repository": "Brightspace/frau-module-loader",
  "bugs": "https://github.com/Brightspace/frau-module-loader/issues",
  "keywords": [
    "frau",
    "d2l",
    "browserify"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "browserify-shim": "^3.8.10"
  },
  "peerDependencies": {
    "browserify": "^11.0.1"
  },
  "scripts": {
    "test": "exit 0"
  },
  "browserify-shim": {
    "requirejs": "global:requirejs",
    "define": "global:define"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
